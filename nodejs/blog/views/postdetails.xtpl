{{!-- 这是引入_layout.xtpl模板 --!}}
{{extend ('./_layout.xtpl')}}

{{#block ('myposts')}}
   <article class="container">
    <div class="row">
      <div class="col-lg-8 col-lg-offset-2 col-md-10 col-md-offset-1">
       
        {{{post.content}}}
      </div>
    </div>
  </article>
  <div class="container">
    <div class="row">
      <div class="col-lg-8 col-lg-offset-2 col-md-10 col-md-offset-1">
        <hr>
        <div class="media">
          <div class="media-left">
            <a href="#">
              <img class="media-object" data-src="holder.js/64x64" alt="64x64" src="data:image/svg+xml;base64," data-holder-rendered="true" style="width: 64px; height: 64px;">
            </a>
          </div>
          <div class="media-body">
            <h4 class="media-heading">Space Ipsum</h4>
            <p class="content">Cras sit amet nibh libero, in gravida nulla. Nulla vel metus scelerisque ante sollicitudin commodo.</p>
            <p class="meta">
              <span><i class="fa fa-clock-o" aria-hidden="true"></i> <em>August 24, 2014</em></span>
              <span class="pull-right">
                <a href="#"><i class="fa fa-thumbs-o-up" aria-hidden="true"></i> 10</a>
                <a href="#"><i class="fa fa-thumbs-o-down" aria-hidden="true"></i> 10</a>
              </span>
            </p>
          </div>
        </div>
        <div class="media">
          <div class="media-left">
            <a href="#">
              <img class="media-object" data-src="holder.js/64x64" alt="64x64" src="data:image/svg+xml;base64," data-holder-rendered="true" style="width: 64px; height: 64px;">
            </a>
          </div>
          <div class="media-body">
            <h4 class="media-heading">Media heading</h4>
            <p class="content">Cras sit amet nibh libero, in gravida nulla. Nulla vel metus scelerisque ante sollicitudin commodo.</p>
            <p class="meta">
              <span><i class="fa fa-clock-o" aria-hidden="true"></i> <em>August 24, 2014</em></span>
              <span class="pull-right">
                <a href="#"><i class="fa fa-thumbs-o-up" aria-hidden="true"></i> 10</a>
                <a href="#"><i class="fa fa-thumbs-o-down" aria-hidden="true"></i> 10</a>
              </span>
            </p>
            <div class="media">
              <div class="media-left">
                <a href="#">
                  <img class="media-object" data-src="holder.js/64x64" alt="64x64" src="data:image/svg+xml;base64," data-holder-rendered="true" style="width: 64px; height: 64px;">
                </a>
              </div>
              <div class="media-body">
                <h4 class="media-heading">Nested media heading</h4>
                <p class="content">Cras sit amet nibh libero, in gravida nulla. Nulla vel metus scelerisque ante sollicitudin commodo.</p>
            <p class="meta">
              <span><i class="fa fa-clock-o" aria-hidden="true"></i> <em>August 24, 2014</em></span>
              <span class="pull-right">
                <a href="#"><i class="fa fa-thumbs-o-up" aria-hidden="true"></i> 10</a>
                <a href="#"><i class="fa fa-thumbs-o-down" aria-hidden="true"></i> 10</a>
              </span>
            </p>
              </div>
            </div>
          </div>
        </div>
        <hr>
        <form action="" class="row">
          <div class="col-md-6 col-xs-12">
            <div class="form-group">
              <label for="name">Name</label>
              <input type="text" name="name" id="name" class="form-control">
            </div>
          </div>
          <div class="col-md-6 col-xs-12">
            <div class="form-group">
              <label for="email">Email</label>
              <input type="text" name="email" id="email" class="form-control">
            </div>
          </div>
          <div class="col-xs-12">
            <div class="form-group">
              <label for="comment">Comment</label>
              <textarea name="comment" id="comment" class="form-control" cols="80" rows="6"></textarea>
            </div>
          </div>
          <div class="col-xs-12">
            <div class="form-group">
              <button class="btn btn-default btn-block">Submit</button>
            </div>
          </div>
        </form>
      </div>
    </div>
  </div>
  <hr>
{{/block}}